
node {
    // Mark the code checkout 'stage'....
    stage 'Checkout'

    // Get some code from a GitHub repository
    //git url: 'https://github.com/VentureTech/proteus-examples.git'
    checkout scm

    // Mark the code build 'stage'....
    stage 'Build'
    // Run the maven build

    sh "./example-app/gradlew clean assemble"
}