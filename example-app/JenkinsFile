


node {
    def jdkHome =tool 'JDK 8'
    env.JAVA_HOME=jdkHome
    // Mark the code checkout 'stage'....
    stage 'Checkout'

    // Get some code from a GitHub repository
    //git url: 'https://github.com/VentureTech/proteus-examples.git'
    checkout scm

    // Mark the code build 'stage'....
    stage 'Build'
    // Run the build
    sh "cd example-app; ./gradlew clean assemble -Prepo_venturetech_username=${env.repo_venturetech_username} " +
        "-Prepo_venturetech_password=${env.repo_venturetech_password} " +
        '-Pcompile_libraries=true --no-daemon --console plain'
}